class Solution {
public:
    ListNode* reverseList(ListNode* head) {
        ListNode* temp=head;
        stack<int>syam;
        while(temp!=nullptr)
            {
                syam.push(temp->val);
                temp=temp->next;
            }
        temp=head;
        while(temp!=nullptr)
            {
                temp->val=syam.top();
                syam.pop();
                temp=temp->next;
            }
        return head;
    }
};
